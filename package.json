{
  "name": "demo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "vite --host",
    "electron:start": "electron electron/main.js",
    "electron:build": "vite build && electron-builder",
    "electron:build:win": "vite build && electron-builder --win --x64"
  },
  "dependencies": {
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "electron": "^35.0.0",
    "electron-builder": "^26.0.13",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.5"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "build": {
    "appId": "com.demo.app",
    "productName": "Demo",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**",
      "electron/**",
      "package.json"
    ],
    "asar": true,
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  }
}
